
<div class="container">
 <h3>All Users</h3>
  <div class="ui list">
    <% @users.each do |user| %>
      <% if user.id != current_user.id %>
        <div class="item">
          <%= link_to user.first_name, chats_path(sender_id: current_user.id, receiver_id: user.id), method: 'post'%>
        </div>
      <% end %>
    <% end %>
 </div>
</div>
